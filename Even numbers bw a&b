import java.util.*;
class even{
 public static void main(String...args){
  try{
  int a,b;
  Scanner sc=new Scanner(System.in);
  System.out.println("Enter the required 1st number :");
  a=sc.nextInt();
  System.out.println("Enter the required 2nd number:");
  b=sc.nextInt();
System.out.println("Even numbers are:");
  for(int i=a;i<b;i++){
   if(i%2==0){
     System.out.print(i+"\t");
    }
}} catch (Exception e){
  System.out.println("Invalid input");}}}
